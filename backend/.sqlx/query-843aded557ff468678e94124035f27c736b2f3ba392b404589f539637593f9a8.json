  "query": "\n            SELECT\n                id,\n                parent_id,\n                (SELECT COUNT(id) FROM comments WHERE parent_id = c.id) as children_count,\n                account_id,\n                content,\n                created_at,\n                updated_at\n            FROM comments c\n            WHERE parent_id = $1\n            ORDER BY created_at DESC, id DESC\n        ",
